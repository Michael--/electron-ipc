{
  "name": "__PACKAGE_NAME__",
  "version": "0.1.0",
  "description": "Electron app powered by @number10/electron-ipc",
  "main": "dist/main/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "mkdirp dist && electron-ipc-generate --config=./src/main/ipc-config.yaml && electron-vite dev",
    "build": "mkdirp dist && electron-ipc-generate --config=./src/main/ipc-config.yaml && electron-vite build",
    "preview": "electron-vite preview",
    "clean": "rimraf dist out *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,html,css}\"",
    "generate": "mkdirp dist && electron-ipc-generate --config=./src/main/ipc-config.yaml",
    "generate:watch": "mkdirp dist && electron-ipc-generate --config=./src/main/ipc-config.yaml --watch",
    "generate:check": "mkdirp dist && electron-ipc-generate --config=./src/main/ipc-config.yaml --check",
    "package": "electron-builder",
    "package:mac": "electron-builder --mac",
    "package:win": "electron-builder --win",
    "package:linux": "electron-builder --linux"
  },
  "keywords": [],
  "author": "__AUTHOR__",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.8",
    "electron": "^35.0.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^2.3.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "mkdirp": "^3.0.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@number10/electron-ipc": "__ELECTRON_IPC_VERSION__",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.1"
  },
  "build": {
    "appId": "__APP_ID__",
    "productName": "__PRODUCT_NAME__",
    "directories": {
      "output": "out"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  }
}
