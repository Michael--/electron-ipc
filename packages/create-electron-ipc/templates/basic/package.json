{
  "name": "__PACKAGE_NAME__",
  "version": "0.1.0",
  "description": "Electron app powered by @number10/electron-ipc",
  "main": "out/main/index.js",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "mkdirp dist && electron-ipc-generate --config=./src/main/ipc-config.yaml && electron-vite dev",
    "build": "mkdirp dist && electron-ipc-generate --config=./src/main/ipc-config.yaml && electron-vite build",
    "preview": "electron-vite preview",
    "clean": "rimraf dist out *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,html,css}\"",
    "generate": "mkdirp dist && electron-ipc-generate --config=./src/main/ipc-config.yaml",
    "generate:watch": "mkdirp dist && electron-ipc-generate --config=./src/main/ipc-config.yaml --watch",
    "generate:check": "mkdirp dist && electron-ipc-generate --config=./src/main/ipc-config.yaml --check",
    "package": "electron-builder",
    "package:mac": "electron-builder --mac",
    "package:win": "electron-builder --win",
    "package:linux": "electron-builder --linux"
  },
  "keywords": [],
  "author": "__AUTHOR__",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.17",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^17.0.0",
    "mkdirp": "^3.0.1",
    "prettier": "^3.8.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@number10/electron-ipc": "__ELECTRON_IPC_VERSION__",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.3.5"
  },
  "build": {
    "appId": "__APP_ID__",
    "productName": "__PRODUCT_NAME__",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  }
}
