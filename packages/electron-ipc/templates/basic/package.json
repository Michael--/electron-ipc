    "clean": "rm -f *.generated.ts ipc-api-main-broadcast.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@number10/electron-ipc": "workspace:*",
    "electron": "^28.0.0",
    "typescript": "^5.3.3"
  }
}
