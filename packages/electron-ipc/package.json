{
  "name": "electron-ipc",
  "version": "0.1.0",
  "description": "TypeScript code generator for type-safe Electron IPC communication",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "watch": "tsc -b --watch",
