  },
  "keywords": [
    "electron",
    "ipc",
    "typescript",
    "generator",
    "type-safe",
    "code-generation"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/electron-ipc.git",
    "directory": "packages/electron-ipc"
  },
  "bugs": {
    "url": "https://github.com/yourusername/electron-ipc/issues"
  },
  "homepage": "https://github.com/yourusername/electron-ipc#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "electron": ">=28.0.0"
  },
  "peerDependenciesMeta": {
    "electron": {
      "optional": true
    }
  }
