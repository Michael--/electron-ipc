{
  "compilerOptions": {
    "lib": ["ES2020", "DOM"],
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["renderer.ts", "preload.ts", "trace-grouping.ts"]
}
