{
  "name": "electron-ipc-test-app",
  "version": "1.0.0",
  "private": true,
  "description": "Test application for electron-ipc library",
  "main": "dist/main/index.js",
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "watch": "tsc -b --watch",
    "dev": "npm run build && electron .",
    "start": "electron .",
    "package": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "electron-ipc": "file:../electron-ipc"
  },
  "build": {
    "appId": "com.example.electron-ipc-test",
    "productName": "Electron IPC Test",
    "directories": {
      "output": "out"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  }
}
