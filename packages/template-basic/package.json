{
  "name": "@number10/template-basic",
  "version": "2.2.0",
  "description": "Basic template for @number10/electron-ipc with code generation",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "electron-ipc-generate --config=ipc-config.yaml",
    "clean": "shx rm -f *.generated.ts ipc-api-main-broadcast.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@number10/electron-ipc": "workspace:*",
    "@types/node": "^25.0.9",
    "electron": "^40.0.0",
    "shx": "^0.4.0",
    "typescript": "^5.9.3"
  }
}
